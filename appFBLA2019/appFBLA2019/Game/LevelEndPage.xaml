<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="appFBLA2019.LevelEndPage">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="LabelScore" Font="Bold"
                   HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" />
            <Button x:Name="ButtonShareToFacebook" Text="Share to Facebook" Clicked="ButtonShareToFacebook_Clicked" />
            <Button x:Name="ButtonShareToOtherMedia" Text="Share to Other Social Media" Clicked="ButtonShareToOtherMedia_Clicked" />
            <Button x:Name="ButtonDone" Text="Done" Clicked="ButtonDone_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>