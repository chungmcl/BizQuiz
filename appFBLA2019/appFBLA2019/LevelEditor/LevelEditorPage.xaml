<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="appFBLA2019.LevelEditorPage">
    <!--Addtional pages may be added programatically into a CarouselPage-->
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="ButtonCreate" Text="Create new level" Clicked="ButtonCreate_Clicked"></Button>
            <Button x:Name="ButtonEdit" Text="Edit existing level" Clicked="ButtonEdit_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>